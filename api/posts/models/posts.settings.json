{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "Slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "thumb": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "count": {
      "type": "integer",
      "default": 0
    },
    "catigory": {
      "via": "posts",
      "model": "catigorys"
    },
    "small": {
      "type": "boolean",
      "default": false
    },
    "medium": {
      "type": "boolean",
      "default": false
    },
    "big": {
      "type": "boolean",
      "default": false
    },
    "secotor": {
      "model": "secotor",
      "via": "posts"
    },
    "news_catigory": {
      "type": "string"
    }
  }
}
