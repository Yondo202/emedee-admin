{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "Thumb": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "count": {
      "type": "integer",
      "default": 0
    },
    "catigory": {
      "model": "catigorys",
      "via": "posts"
    },
    "Layout": {
      "type": "dynamiczone",
      "components": [
        "components.othernewsbig"
      ]
    },
    "secotor": {
      "via": "posts",
      "model": "secotor"
    }
  }
}
